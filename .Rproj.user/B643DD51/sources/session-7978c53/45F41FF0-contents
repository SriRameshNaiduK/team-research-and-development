library(ggplot2)
library(gridExtra)
data <- read.csv("final_data.csv")
contingency_table <- table(data$Designation, data$Involvement)
chi_square_result <- chisq.test(contingency_table)
print(chi_square_result)
top_designations <- head(sort(table(data$Designation), decreasing = TRUE), 50)
selected_designations <- names(top_designations)
filtered_data <- subset(data, Designation %in% selected_designations)
proportions <- as.data.frame(prop.table(table(filtered_data$Designation, filtered_data$Involvement), margin = 1))
colnames(proportions) <- c("Designation", "Involvement", "Proportion")
ggplot(proportions, aes(x = Designation, y = Proportion, fill = Involvement)) +
geom_bar(stat = "identity", position = "dodge") +
labs(title = "Grouped Bar Chart: Involvement Proportions Across Top 50 Designations",
x = "Designation",
y = "Proportion",
fill = "Involvement") +
theme_minimal() +
theme(axis.text.x = element_text(angle = 45, hjust = 1),
plot.title = element_text(hjust = 0.5, size = 14, face = "bold"))
ggsave("Stacked_Bar_Chart_Involvement_Proportions_Across_Top_50_Designations.png", width = 15, height = 5.677, bg = "white")
